<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:enterFadeDuration="200" android:exitFadeDuration="200">
    <item android:state_pressed="true" android:drawable="@drawable/func_map_shape_btn_bg_select"/>
    <item android:state_enabled="false">
        <shape
            android:shape="rectangle">
            <solid android:color="@color/util_theme_color_app_text_gray" />
            <corners android:radius="@dimen/func_map_btn_bg_corner" />
            <stroke
                android:width="@dimen/func_map_btn_border_w"
                android:color="@color/util_theme_color_app_text_gray" />
        </shape>
    </item>
    <item android:drawable="@drawable/func_map_shape_btn_bg_normal"/>
</selector>